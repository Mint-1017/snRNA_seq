   "source": [
    "#PTS1 file\n",
    "PTS1 = []\n",
    "\n",
    "meta = []\n",
    "Ext_meta = \"/path/to/Extract_4hours_GSE139107_MouseIRI.metadata.txt\"\n",
    "\n",
    "Ext_meta_f = open(Ext_meta,'r',encoding='utf-8')\n",
    "for line in Ext_meta_f:\n",
    "    meta.append(line.split())\n",
    "\n",
    "for line in meta:\n",
    "    if line[3] == 'PTS1':\n",
    "        PTS1.append(line[0]) \n",
    "        \n",
    "f = open('SpeedTest_4hours_PTS1.txt','w')\n",
    "\n",
    "for line in PTS1:\n",
    "    f.write(line)\n",
    "    f.write('\\t')\n",
    "f.write('\\n')\n",
    "\n",
    "four = \"/path/to/GSE139107_MouseIRI_4hours.dge.txt\"\n",
    "four_f = open(four,'r',encoding='utf-8')\n",
    "\n",
    "l = []\n",
    "for line in four_f:\n",
    "    l.append(line.split())\n",
    "\n",
    "\n",
    "for j in range(1,27134):\n",
    "    f.write(l[j][0])\n",
    "    f.write('\\t')\n",
    "    k = 0\n",
    "    for line in PTS1:\n",
    "        for i in range(k,14486): \n",
    "            if line == l[0][i]:\n",
    "                f.write(l[j][i+1])\n",
    "                f.write('\\t')\n",
    "                k = i\n",
    "                break\n",
    "    f.write('\\n')\n",
    "    \n",
    "f.close()"
   ]
